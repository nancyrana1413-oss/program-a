// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int age;
    
    cout << "enter your age:";
    cin >> age;
    if (age < 12)
        cout << "child" <<endl;
    else if (age < 18)    
        cout <<"teenager" <<endl;
    else
        cout <<"young" <<endl;
    return 0;
}
